// EXCERCISE 8: GUESS THE NUMBER GAME
// This was exhausting. But we do hard things ðŸ˜Š

// Instruction
for (let index = 1; index <= 15; index++) {
  if (index % 3 === 0 && index % 5 === 0) {
    console.log("FizzBuzz");
  } else if (index % 3 === 0) {
    console.log("Fizz");
  } else if (index % 5 === 0) {
    console.log("Buzz");
  } else console.log(index);
}
